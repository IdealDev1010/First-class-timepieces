      "about-us": {
        "type": "about-us",
        "blocks": {
          "1546731782413": {
            "type": "image-hero",
            "settings": {
              "parallax": false,
              "title": "Founded with a mission.",
              "title_size": 80,
              "subheading": "To provide quality watches at affordable prices",
              "link": "",
              "link_text": "",
              "link_2": "",
              "link_text_2": "",
              "color_accent": "rgba(0,0,0,0)",
              "text_align": "vertical-center horizontal-center",
